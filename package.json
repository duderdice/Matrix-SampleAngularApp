{
  "name": "sample-angular-app",
  "version": "0.0.2",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "ng build --env=dev",
    "build:mock": "ng build --env=mock",
    "build:prod": "ng build --env=prod",
    "clean": "rimraf -- dist/* node_modules",
    "clean:dist": "rimraf -- dist/*",
    "e2e": "ng e2e",
    "generate:deployment-script": "kuduscript -y --node",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild": "npm run clean:dist",
    "start": "npm run lint && ng serve",
    "start:mock": "npm run lint && ng serve --env=mock",
    "start:mock:nolint": "ng serve --env=mock",
    "test": "npm run lint && ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~5.2.0",
    "@angular/common": "~5.2.0",
    "@angular/compiler": "~5.2.0",
    "@angular/core": "~5.2.0",
    "@angular/forms": "~5.2.0",
    "@angular/http": "~5.2.0",
    "@angular/platform-browser": "~5.2.0",
    "@angular/platform-browser-dynamic": "~5.2.0",
    "@angular/router": "~5.2.0",
    "@ngrx/effects": "4.1.1",
    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.1.1",
    "angular2-moment": "1.7.0",
    "angular2-toaster": "4.0.1",
    "core-js": "2.5.1",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.0.26",
    "@angular/cli": "1.6.4",
    "@angular/compiler-cli": "5.2.1",
    "@angular/language-service": "5.2.1",
    "@types/jasmine": "2.5.45",
    "@types/node": "~8.0.47",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "kuduscript": "^1.0.15",
    "protractor": "~5.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "~3.3.0",
    "tslint": "~5.9.0",
    "typescript": "~2.4.2"
  }
}